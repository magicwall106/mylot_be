extends ../layout

block content
  h2 Results Viet Lottery
  hr

  .row
    p #{listResult}
    table.table
      thead
        tr
          th Code
          th Budget
          th Result Date
      tbody
        each val in listResult
          tr
            td= val.code
            td= val.budget
            td= val.resultDate
        else
          tr There no values
    a(href='/result/add', style='color: #fff')
      .panel.panel-default(style='background-color: #3b5998')
        .panel-body
          |  Add Result
  
